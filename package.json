{
	"name": "@gurisandhu/play-ui",
	"version": "0.0.23",
	"scripts": {
		"sb": "start-storybook -p 6005 -s ./public --no-open",
		"build:sb": "build-storybook -s public",
		"build": "tsup",
		"lint": "tsc",
		"release": "standard-version",
		"format": "prettier --write \"./**/*.{ts,tsx,md,json}\" --config ./.prettierrc"
	},
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts",
			"default": "./dist/index.js"
		},
		"./helpers": {
			"import": "./dist/helpers/index.js",
			"types": "./dist/helpers/index.d.ts",
			"default": "./dist/helpers/index.js"
		},
		"./types": {
			"import": "./dist/types/index.js",
			"types": "./dist/types/index.d.ts",
			"default": "./dist/types/index.js"
		},
		"./utils": {
			"import": "./dist/utils/index.js",
			"types": "./dist/utils/index.d.ts",
			"default": "./dist/utils/index.js"
		},
		"./atoms": {
			"import": "./dist/atoms/index.js",
			"types": "./dist/atoms/index.d.ts",
			"default": "./dist/atoms/index.js"
		},
		"./molecules": {
			"import": "./dist/molecules/index.js",
			"types": "./dist/molecules/index.d.ts",
			"default": "./dist/molecules/index.js"
		}
	},
	"dependencies": {
		"@types/node": "20.3.1",
		"@types/react": "18.2.14",
		"@types/react-dom": "18.2.6",
		"@typescript-eslint/eslint-plugin": "^5.60.0",
		"classnames": "^2.3.2",
		"date-fns": "^2.30.0",
		"draft-js": "^0.11.7",
		"eslint": "8.43.0",
		"eslint-config-next": "13.4.7",
		"next": "13.4.7",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"react-responsive": "^9.0.2",
		"react-swipeable": "^7.0.1",
		"styled-components": "6.0.0-rc.6",
		"typescript": "5.1.3",
		"underscore": "^1.13.6"
	},
	"devDependencies": {
		"@babel/core": "7.20.5",
		"@storybook/addon-actions": "^6.5.16",
		"@storybook/addon-essentials": "^6.5.16",
		"@storybook/addon-interactions": "^6.5.16",
		"@storybook/addon-links": "^6.5.16",
		"@storybook/builder-webpack5": "^6.5.16",
		"@storybook/manager-webpack5": "^6.5.16",
		"@storybook/nextjs": "7.0.20",
		"@storybook/react": "^6.5.16",
		"@storybook/testing-library": "^0.0.13",
		"@svgr/webpack": "^6.5.1",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.4.0",
		"@types/draft-js": "^0.11.12",
		"@types/eslint": "^8.40.2",
		"@types/jest": "^29.5.2",
		"@types/styled-components": "^5.1.26",
		"@types/underscore": "^1.11.5",
		"babel-loader": "^8.3.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-storybook": "^0.6.12",
		"jest": "^29.5.0",
		"jest-environment-jsdom": "^29.5.0",
		"prettier": "^2.8.8",
		"standard-version": "^9.5.0",
		"tsconfig-paths-webpack-plugin": "^4.0.1",
		"tsup": "^7.1.0",
		"webpack": "^5.88.0"
	},
	"peerDependencies": {
		"react": "18.2.0",
		"react-dom": "18.2.0"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/gurisandhu/play-ui"
	},
	"author": "Guri Sandhu",
	"license": "ISC",
	"homepage": "https://github.com/gurisandhu/play-ui#readme",
	"publishConfig": {
		"@gurisandhu:registry": "https://npm.pkg.github.com"
	},
	"bugs": {
		"url": "https://github.com/gurisandhu/play-ui/issues"
	}
}
